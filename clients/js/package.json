{
    "name": "aishe-client",
    "version": "1.0.0",
    "description": "AIshe JavaScript/TypeScript client",
    "main": "dist/src/index.js",
    "types": "dist/src/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/src/index.js",
            "types": "./dist/src/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsc",
        "start": "node dist/src/main.js",
        "lint": "eslint . --ext js,ts",
        "lint:fix": "eslint . --ext js,ts --fix",
        "format": "prettier . --write",
        "format:check": "prettier . --check",
        "session1": "npm run build && node dist/workshop/session1-basic/starter/main.js",
        "session2": "npm run build && node dist/workshop/session2-redis/starter/main.js",
        "session3": "npm run build && node dist/workshop/session3-langcache/starter/main.js",
        "session1:starter": "npm run build && node dist/workshop/session1-basic/starter/main.js",
        "session2:starter": "npm run build && node dist/workshop/session2-redis/starter/main.js",
        "session3:starter": "npm run build && node dist/workshop/session3-langcache/starter/main.js",
        "session1:solution": "npm run build && node dist/workshop/session1-basic/solution/main.js",
        "session2:solution": "npm run build && node dist/workshop/session2-redis/solution/main.js",
        "session3:solution": "npm run build && node dist/workshop/session3-langcache/solution/main.js"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "type": "module",
    "devDependencies": {
        "@types/node": "^24.10.1",
        "@typescript-eslint/eslint-plugin": "^8.48.1",
        "@typescript-eslint/parser": "^8.48.1",
        "eslint": "^9.39.1",
        "prettier": "^3.7.3",
        "typescript": "^5.9.3"
    },
    "dependencies": {
        "@redis-ai/langcache": "^0.11.1",
        "redis": "^5.10.0"
    }
}
